		<queryString language="SQL">
			<![CDATA[select 
	ROW_NUMBER() OVER (ORDER by df.id_detalle_factura) as "id",
	df.id_detalle_factura,
	df.cantidad,
	cum.descripcion_medida,
	df.nombre,
	df.precio_unitario,
	0 as "no_grabado",
	0 as "descuento",
	0 as "no_sujeta",
	case f.codigo_dte 
		when '14' then df.cantidad * df.precio_unitario
		else 0
	end as "exenta",
	case f.codigo_dte 
		when '14' then 0
		else df.cantidad * df.precio_unitario
	end as "grabado"
from factura f 
	inner join detalle_factura df on f.id_factura = df.id_factura
	inner join cat_014_unidad_medida cum on df.codigo_unidad = cum.codigo_unidad
where
	f.id_factura =   $P{PID_FACT} 
order by df.id_detalle_factura asc]]>
		</queryString>
