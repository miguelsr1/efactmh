	<queryString language="SQL">
		<![CDATA[select 
	UPPER(c.nombre_comercial) nombre,
	c.nit,
	c.nrc,
	ae.actividad_economica,
	UPPER(c.direccion) direccion, 
	m.nombre_municipio, 
	d.nombre_departamento,
	c.telefono,
	c.correo,
	ctd.nombre_dte,
	f.codigo_generacion,
	f.numero_control,
	cmf.descripcion,
	ctt.descirpionc,
	TO_CHAR(f.fecha_creacion,'dd/MM/yyyy') fecha,
	TO_CHAR(f.fecha_creacion,'HH:mm:ss') hora,
	upper(numero_a_texto2((
	CASE 
    	WHEN f.codigo_dte = '01' THEN round(sum(df.cantidad * df.precio_unitario),2)
    	WHEN f.codigo_dte  = '03' THEN (round(sum(df.cantidad * df.precio_unitario) + (sum(df.cantidad * df.precio_unitario) * 0.13) - round(sum(df.cantidad * df.precio_unitario) *(
	CASE 
    	WHEN f.retencion_isr = 0 THEN 0
    	WHEN f.retencion_isr = 1 THEN 0.01
    	ELSE 0.1
  	END) ,2) ,2)    			
    	)
    	ELSE 0.1
  	END
	) - round(
	CASE f.aplica_iva_retenido 
    	WHEN true THEN (sum(df.cantidad * df.precio_unitario)/1.13)*0.01
    	ELSE 0.00
  	END ,2))) valor_letras,
	vw.recep_nombre,
	vw.recep_actividad,
	vw.recep_direccion,
	vw.recep_municipio,
	vw.recep_departamento,
	vw.recep_tipo_documento,
	vw.recep_numero_documento,
	vw.recep_nrc,
	vw.recep_telefono,
	vw.recep_correo,
	vw.id_cliente,
	sum(df.cantidad * df.precio_unitario) sumas,
	(
	CASE 
    	WHEN f.codigo_dte = '01' THEN 0
    	WHEN f.codigo_dte  = '03' THEN round(sum(df.cantidad * df.precio_unitario) * 0.13,2)
    	when f.codigo_dte = '14' then 0
    	ELSE 0.1
  	END
	) iva,
	(
	CASE 
    	WHEN f.codigo_dte = '01' THEN 0
    	WHEN f.codigo_dte  = '03' THEN round(sum(df.cantidad * df.precio_unitario) + round(sum(df.cantidad * df.precio_unitario) * 0.13,2),2)
    	when f.codigo_dte = '14' then 0
    	ELSE 0.1
  	END
	) sub_total,
	round(sum(df.cantidad * df.precio_unitario) *(
	CASE 
    	WHEN f.retencion_isr = 0 THEN 0
    	WHEN f.retencion_isr = 1 THEN 0.01
    	ELSE 0.1
  	END) ,2) isr_retenido,
  	round(
	CASE f.aplica_iva_retenido 
    	WHEN true THEN (sum(df.cantidad * df.precio_unitario)/1.13)*0.01
    	ELSE 0.00
  	END ,2) iva1_retenido,
	(
	CASE 
    	WHEN f.codigo_dte = '01' THEN round(sum(df.cantidad * df.precio_unitario),2)
    	WHEN f.codigo_dte  = '03' THEN (round(sum(df.cantidad * df.precio_unitario) + (sum(df.cantidad * df.precio_unitario) * 0.13) - round(sum(df.cantidad * df.precio_unitario) *(
	CASE 
    	WHEN f.retencion_isr = 0 THEN 0
    	WHEN f.retencion_isr = 1 THEN 0.01
    	ELSE 0.1
  	END) ,2) ,2)    			
    	)
    	when f.codigo_dte = '14' then round(sum(df.cantidad * df.precio_unitario),2)
    	ELSE 0.1
  	END
	) - round(
	CASE f.aplica_iva_retenido 
    	WHEN true THEN (sum(df.cantidad * df.precio_unitario)/1.13)*0.01
    	ELSE 0.00
  	END ,2) total ,
	concat('https://admin.factura.gob.sv/consultaPublica?ambiente=',f.ambiente,'&codGen=',f.codigo_generacion,'&fechaEmi=', TO_CHAR(bm.fh_procesamiento,'yyyy-mm-dd')) qr
from 
	contribuyente c 
	inner join factura f on c.id_contribuyente = f.id_contribuyente
	inner join bitacora_mh bm on f.id_factura = bm.id_factura and bm.activo = true and bm.tipo_dte = 'F'
	inner join detalle_factura df on f.id_factura = df.id_factura 
	inner join cat_002_tipo_documentos ctd on ctd.codigo_dte = f.codigo_dte
	inner join cat_003_modelo_facturacion cmf on f.codigo_facturacion = cmf.codigo_facturacion
	inner join cat_004_tipo_transmicion ctt on f.codigo_transmision = ctt.codigo_transmision
	inner join actividad_economica ae on c.codigo_actividad = ae.codigo_actividad
	inner join usuario u on c.usuario = u.correo_electronico
	inner join municipio m on c.id_municipio = m.id_municipio
	inner join departamento d on m.codigo_departamento = d.codigo_departamento
	inner join vw_receptor_dte vw on f.id_cliente = vw.id_cliente
where 
	f.id_factura = $P{PID_FACTURA} 
group by
	c.nombre_comercial,
	c.nit,
	c.nrc,
	ae.actividad_economica,
	c.direccion, 
	m.nombre_municipio, 
	d.nombre_departamento,
	c.telefono,
	c.correo,
	ctd.nombre_dte,
	f.codigo_generacion,
	f.numero_control,
	f.aplica_iva_retenido ,
	cmf.descripcion,
	ctt.descirpionc,
	f.fecha_creacion,
	f.fecha_creacion,
	f.monto_operacion,
	vw.recep_nombre,
	vw.recep_actividad,
	vw.recep_direccion,
	vw.recep_municipio,
	vw.recep_departamento,
	vw.recep_tipo_documento,
	vw.recep_numero_documento,
	vw.recep_nrc,
	vw.recep_telefono,
	vw.recep_correo,
	vw.id_cliente,
	f.retencion_isr,
	f.codigo_dte,
	f.ambiente,
	bm.fh_procesamiento]]>
	</queryString>
