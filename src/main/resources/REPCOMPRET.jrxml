		<parameter name="PNRO_FACTURA" class="java.lang.String"/>
		<queryString language="SQL">
			<![CDATA[SELECT DISTINCT
    fac_fecha_emision,
    DATE_FORMAT(fac_fecha_emision, '%Y') AS year,
    DATE_FORMAT(fac_fecha_emision, '%m') AS month,
    DATE_FORMAT(fac_fecha_emision, '%d') AS day,
    DATE_FORMAT(fac_fecha_emision, '%H:%i:%s') AS hour
FROM
    facturacion_electronica.factura
INNER JOIN
    bitacora_declaracion_hacienda ON id_fac = fac_id
WHERE
    fac_id = $P{PNRO_FACTURA}]]>
